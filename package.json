{
  "name": "ngraph.svg",
  "version": "0.12.0",
  "description": "SVG-based graph visualization library with adaptive rendering",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite --config vite.demo.config.js",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.js && cp demo/hypernym-dag-v2.json docs/",
    "preview": "vite preview --config vite.demo.config.js",
    "bench:layout": "node bench/force-layout.benchmark.js",
    "bench:layout:layered": "node bench/force-layout-layered.benchmark.js",
    "bench": "node bench/run-layout-benches.js"
  },
  "dependencies": {
    "ngraph.forcelayout": "^3.3.1",
    "ngraph.generators": "^21.0.0",
    "ngraph.graph": "^20.1.1",
    "rbush": "^3.0.1"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "ngraph.fromdot": "^7.1.0",
    "ngraph.louvain": "^2.0.0",
    "vite": "^6.0.11"
  },
  "keywords": [
    "graph",
    "visualization",
    "svg",
    "ngraph",
    "adaptive"
  ],
  "author": "",
  "license": "MIT"
}
